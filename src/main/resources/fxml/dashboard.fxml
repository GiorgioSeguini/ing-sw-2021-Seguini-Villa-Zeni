<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="anchorPane" prefHeight="692.0" prefWidth="1280.0" styleClass="basePane" stylesheets="@Style.css"
            xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="it.polimi.ingsw.client.gui.DashboardController">
    <Button fx:id="baseButton" layoutX="14.0" layoutY="1.0" mnemonicParsing="false" onAction="#baseButton"
            prefHeight="22.0" prefWidth="24.0" styleClass="baseButton" text="X"/>
    <Button fx:id="confirmButton" layoutX="48.0" layoutY="1.0" mnemonicParsing="false" onAction="#confirm"
            styleClass="baseButton" text="Compra"/>
    <ImageView fx:id="imageViewBoard" layoutX="50.0" layoutY="50.0" pickOnBounds="true" preserveRatio="true">
        <Image url="@../images/board/Masters_of_Renaissance_PlayerBoard_DevCard.jpg"/>
    </ImageView>
    <ImageView fx:id="devcardBuyed" fitHeight="242.0" fitWidth="233.0" layoutX="1000.0" layoutY="139.0"
               pickOnBounds="true" preserveRatio="true"/>
    <ImageView fx:id="imageViewDepots" layoutX="1000.0" layoutY="139.0" pickOnBounds="true" preserveRatio="true">
        <Image url="@../images/board/Depots.png"/>
    </ImageView>
</AnchorPane>
